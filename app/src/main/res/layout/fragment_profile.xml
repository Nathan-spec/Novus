<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Orange_Red"
    tools:context=".ProfileFragment">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <!-- First Card-->
            <androidx.cardview.widget.CardView
                android:id="@+id/first_card"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/image_one"
                        android:layout_width="56dp"
                        android:layout_height="52dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.095"
                        app:layout_constraintStart_toStartOf="@+id/textView"
                        tools:ignore="MissingConstraints"
                        tools:layout_editor_absoluteY="3dp"
                        tools:src="@drawable/ic_baseline_assignment_ind_24" />

                    <TextView
                        android:id="@+id/textView"
                        android:layout_width="match_parent"
                        android:layout_height="59dp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="First"
                        tools:ignore="MissingConstraints"
                        tools:layout_editor_absoluteX="0dp"
                        tools:layout_editor_absoluteY="0dp" />
                </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.cardview.widget.CardView>

            <!-- Second Card -->
            <androidx.cardview.widget.CardView
                android:id="@+id/second_card"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@color/Orange_Red"
                android:paddingTop="5dp"
                app:layout_constraintTop_toBottomOf="@+id/first_card">


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="56dp"
                        android:id="@+id/second_image"
                        android:layout_height="56dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.098"
                        app:layout_constraintStart_toStartOf="@+id/textView2"
                        tools:ignore="MissingConstraints"
                        tools:layout_editor_absoluteY="16dp"
                        tools:src="@drawable/ic_baseline_biotech_24">

                    </ImageView>

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="match_parent"
                        android:layout_height="85dp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Second"
                        tools:ignore="MissingConstraints"
                        tools:layout_editor_absoluteX="0dp"
                        tools:layout_editor_absoluteY="0dp" />

                </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.cardview.widget.CardView>

            <!-- Third card -->
            <androidx.cardview.widget.CardView
                android:id="@+id/sth"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@color/Orange_Red"
                android:paddingTop="5dp"
                app:layout_constraintTop_toBottomOf="@+id/second_card">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="56dp"
                        android:id="@+id/third_image"
                        android:layout_height="56dp"
                        app:layout_constraintEnd_toEndOf="@+id/textView3"
                        app:layout_constraintHorizontal_bias="0.095"
                        app:layout_constraintStart_toStartOf="@+id/textView3"
                        tools:ignore="MissingConstraints"
                        tools:layout_editor_absoluteY="13dp"
                        tools:src="@drawable/ic_baseline_audiotrack_24">

                    </ImageView>

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="match_parent"
                        android:layout_height="85dp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Third"
                        tools:ignore="MissingConstraints" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <!-- Fourth card -->
            <androidx.cardview.widget.CardView
                android:id="@+id/fourth"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@color/Orange_Red"
                app:layout_constraintTop_toBottomOf="@+id/sth">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="56dp"
                        android:id="@+id/fourth_image"
                        android:layout_height="56dp"
                        app:layout_constraintEnd_toEndOf="@+id/textView4"
                        app:layout_constraintHorizontal_bias="0.101"
                        app:layout_constraintStart_toStartOf="parent"
                        tools:ignore="MissingConstraints"
                        tools:layout_editor_absoluteY="9dp"
                        tools:src="@drawable/ic_baseline_bedtime_24" />

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Fourth"
                        tools:ignore="MissingConstraints" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <!-- Fifth card -->
            <androidx.cardview.widget.CardView
                android:id="@+id/fifth"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@color/Orange_Red"
                app:layout_constraintTop_toBottomOf="@+id/fourth">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="56dp"
                        android:id="@+id/fifth_image"
                        android:layout_height="56dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.095"
                        app:layout_constraintStart_toStartOf="@+id/textView5"
                        tools:ignore="MissingConstraints"
                        tools:layout_editor_absoluteY="9dp"
                        tools:src="@drawable/ic_baseline_delete_24" />

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Fifth"
                        tools:ignore="MissingConstraints" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <!-- Sixth card -->
            <androidx.cardview.widget.CardView
                android:id="@+id/sixth"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@color/Orange_Red"
                app:layout_constraintTop_toBottomOf="@+id/fifth">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="56dp"
                        android:id="@+id/sixth_image"
                        android:layout_height="56dp"
                        app:layout_constraintEnd_toEndOf="@+id/textView6"
                        app:layout_constraintHorizontal_bias="0.098"
                        app:layout_constraintStart_toStartOf="parent"
                        tools:ignore="MissingConstraints"
                        tools:layout_editor_absoluteY="9dp"
                        tools:src="@drawable/ic_baseline_donut_small_24" />

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Sixth"
                        tools:ignore="MissingConstraints" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <!-- Seventh card -->
            <androidx.cardview.widget.CardView
                android:id="@+id/seventh"
                android:layout_width="match_parent"
                android:layout_height="130dp"
                android:background="@color/Orange_Red"
                app:layout_constraintTop_toBottomOf="@+id/sixth">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="56dp"
                        android:id="@+id/seventh_image"
                        android:layout_height="56dp"
                        app:layout_constraintEnd_toEndOf="@+id/textView7"
                        app:layout_constraintHorizontal_bias="0.109"
                        app:layout_constraintStart_toStartOf="parent"
                        tools:ignore="MissingConstraints"
                        tools:layout_editor_absoluteY="37dp"
                        tools:src="@drawable/ic_baseline_donut_small_24" />

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Seventh"
                        tools:ignore="MissingConstraints"
                        tools:layout_editor_absoluteX="0dp"
                        tools:layout_editor_absoluteY="28dp" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>

</FrameLayout>